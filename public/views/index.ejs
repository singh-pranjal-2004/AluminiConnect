  <script>
    function toggleAnswer(question) {
      const answer = question.nextElementSibling;
      const arrow = question.querySelector('.arrow');
      if (answer.style.display === 'block') {
        answer.style.display = 'none';
        arrow.classList.remove('open');
      } else {
        answer.style.display = 'block';
        arrow.classList.add('open');
      }
    }

    // =======> START For changing the color of connect, learn, and grow <========
    document.addEventListener('DOMContentLoaded', (event) => {
      const words = ['connect', 'learn', 'succeed'];
      const highlightClasses = ['highlight-connect', 'highlight-learn', 'highlight-succeed'];
      let currentIndex = 0;

      function changeColor() {
        words.forEach((wordId, index) => {
          document.getElementById(wordId).classList.remove(highlightClasses[index]);
        });

        document.getElementById(words[currentIndex]).classList.add(highlightClasses[currentIndex]);

        currentIndex = (currentIndex + 1) % words.length;
      }

      setInterval(changeColor, 1000);
    });
    // =======> END For changing the color of connect, learn, and grow <========

  </script>
